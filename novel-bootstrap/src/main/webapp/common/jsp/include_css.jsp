<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%
	String path = request.getContextPath();
	String basePath = request.getScheme() + "://"
			+ request.getServerName() + ":" + request.getServerPort()
			+ path + "/";
%>

	<!-- required styles -->
	<link type="text/css" rel="stylesheet" href="<%=basePath%>common/css/easyui/themes/default/easyui.css"/>
	<link type="text/css" rel="stylesheet" href="<%=basePath%>common/css/easyui/themes/icon.css"/>
	<link type="text/css" rel="stylesheet" href="<%=basePath%>common/css/common.css"/>
	
	